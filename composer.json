{
    "name": "neepco/asset-management-system",
    "type": "project",
    "description": "Asset Management System For NEEPCO LTD - A comprehensive solution for managing organizational assets",
    "homepage": "https://github.com/Pratik-Dev-Codes/Asset-Management-System-For-NEEPCO-LTD",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "barryvdh/laravel-dompdf": "^3.1",
        "darkaonline/l5-swagger": "^8.6",
        "doctrine/dbal": "^3.9",
        "inertiajs/inertia-laravel": "^0.6.3",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.2",
        "laravel/telescope": "*",
        "maatwebsite/excel": "^3.1",
        "milon/barcode": "*",
        "pusher/pusher-php-server": "*",
        "simplesoftwareio/simple-qrcode": "*",
        "spatie/laravel-activitylog": "*",
        "spatie/laravel-permission": "^6.18",
        "tightenco/ziggy": "^2.0",
        "zircote/swagger-php": "^4.11"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.29",
        "laravel/dusk": "*",
        "laravel/pint": "*",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.5",
        "spatie/laravel-ignition": "^2.0",
        "zircote/swagger-php": "^4.11"
    },

    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "App\\Http\\Controllers\\API\\": "app/Http/Controllers/API/",
            "App\\Http\\Controllers\\Api\\": "app/Http/Controllers/API/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php -d memory_limit=-1 artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        },
        "process-timeout": 2000
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-dompdf",
                "darkaonline/l5-swagger",
                "inertiajs/inertia-laravel",
                "laravel/telescope",
                "maatwebsite/excel",
                "milon/barcode",
                "spatie/laravel-activitylog",
                "spatie/laravel-permission"
            ]
        }
    }
}
